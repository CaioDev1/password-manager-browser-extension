{
    "name": "Password manager",
    "description": "Password manager and suggester for your web browser",
    "version": "1.0",
    "author": "CaioDev1",
    "manifest_version": 3,
    "permissions": ["activeTab", "declarativeContent"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["manager.js"]
        }
    ]
  }
  